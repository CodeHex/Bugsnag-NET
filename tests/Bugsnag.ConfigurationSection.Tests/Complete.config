<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="bugsnag" type="Bugsnag.ConfigurationSection.Configuration, Bugsnag.ConfigurationSection" />
  </configSections>
  <bugsnag
    apiKey="abc123"
    appType="test"
    autoNotify="false"
    appVersion="1.0"
    notify="https://www.bugsnag.com"
    notifyReleaseStages="development,test,production"
    releaseStage="test"
    projectRoots="C:\app,D:\src"
    projectNamespaces="App.Code,Bugsnag.Tests"
    metadataFilters="password,creditcard"
    autoCaptureSessions="true"
    proxyAddress="https://bugsnag.com:8080"
    proxyUsername="bugsnag"
    proxyPassword="password"
    sessions="https://www.bugsnag.com"
    maximumBreadcrumbs="30">
    <ignoreClasses>
      <class name="System.NotImplementedException" />
      <class name="System.DllNotFoundException" />
      <class name="Bugsnag.NoSuchException" />
      <class name="Xunit.FactAttribute, xunit.core" />
    </ignoreClasses>
    <metadata>
      <item key="test" value="wow" />
    </metadata>
  </bugsnag>
</configuration>
